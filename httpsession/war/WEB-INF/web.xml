<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	
	<servlet>
		<servlet-name>Httpsession</servlet-name>
		<servlet-class>com.ds.HttpsessionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Httpsession</servlet-name>
		<url-pattern>/httpsession</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>invalidate</servlet-name>
		<servlet-class>com.ds.invalidate</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>invalidate</servlet-name>
		<url-pattern>/invalidate</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.ds.login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>httpsession</welcome-file>
	</welcome-file-list>
</web-app>
